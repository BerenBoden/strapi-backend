{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "image_header": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "content": {
      "type": "richtext"
    },
    "article_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article-category.article-category",
      "mappedBy": "category_articles"
    },
    "article_tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article-tag.article-tag",
      "mappedBy": "tag_articles"
    },
    "author": {
      "type": "string"
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    }
  }
}
